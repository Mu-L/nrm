{
  "name": "nrm",
  "version": "1.4.0",
  "description": "npm registry manager can help you easy and fast switch between different npm registries, now include: cnpm, taobao, nj(nodejitsu), edunpm",
  "bin": {
    "nrm": "bin/cli.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pana/nrm.git"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "star": "npm star nrm",
    "test": "vitest run",
    "build": "tsup",
    "test:watch": "vitest",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "check": "npx @biomejs/biome check --write ."
  },
  "keywords": [
    "npm",
    "registry"
  ],
  "author": "Pana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pana/nrm/issues"
  },
  "homepage": "https://github.com/Pana/nrm",
  "dependencies": {
    "@inquirer/select": "^4.0.2",
    "chalk": "^5.3.0",
    "commander": "^8.3.0",
    "fs-extra": "^11.2.0",
    "ini": "4.1.3",
    "open": "^10.1.0",
    "undici": "5.28.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.8",
    "@types/ini": "^4.1.1",
    "coffee": "^5.4.0",
    "mock-fs": "^5.4.1",
    "tsup": "8.3.0",
    "typescript": "^5.7.2",
    "vitest": "2.1.6"
  },
  "engines": {
    "node": ">=18"
  }
}